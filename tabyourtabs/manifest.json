{
  "applications": {
    "gecko": {
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
      "scripts": ["background.js","d3.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "TabYourTabs",
    "default_popup": "tabs.html"
  },

  "description": "A list of methods you can perform on a tab.",
  "homepage_url": "https://github.com/RiaPant/TabYourTabs",
  "manifest_version": 2,
  "name": "Tabs, tabs, tabs",
  "permissions": [
    "tabs","activeTab"
  ],
  "version": "1.0"
}
